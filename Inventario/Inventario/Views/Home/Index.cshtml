@{
    ViewBag.Title = "Home Page";
}


    <center>
        <h1 >
            <u style="color:red">
                <i style="font-family:'Malgun Gothic'">  SISTEMA DE INVENTARIO</i>
            </u>
        </h1>
    </center>
<br /><br /><br /><br />
    <div id="myCarousel" class="carousel slide" data-ride="carousel" style="background-color:#f1f3f4">
        <!-- Indicadores -->
        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1"></li>
            <li data-target="#myCarousel" data-slide-to="2"></li>
        </ol>

        <!-- esqueleto del carrusel -->
        <div class="carousel-inner"  >
            <div class="item active">
                <center>
                    <img src="https://retos-operaciones-logistica.eae.es/wp-content/uploads/2014/09/fansi026444.jpg" alt="s1">
                </center>
            </div>

            <div class="item">
                <center>
                    <img src="http://www.tradinggroup.com.co/wp-content/uploads/2018/07/toma-profesional-de-inventarios.jpg" alt="s2">
                </center>
            </div>

            <div class="item">
                <center>
                <img src="http://canalferretero.mx/wp-content/uploads/2018/04/B_tf73_Warehouse-Image-copy-1-600x400.jpg" alt="s3">
                </center>
            </div>
        </div>

        <!-- controles de izquierda y derecha -->
        <a class="left carousel-control" href="#myCarousel" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#myCarousel" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>